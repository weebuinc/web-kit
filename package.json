{
  "name": "@weebu/web-kit",
  "version": "0.0.0",
  "description": "Weebu Web Utility Kit",
  "main": "lib/index",
  "types": "index",
  "private": true,
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf lib/*",
    "format": "./node_modules/.bin/prettier --write \"src/**/*.ts\"",
    "test": "./node_modules/.bin/jest",
    "coverage": "./node_modules/.bin/jest --coverage",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build -o docs-build",
    "preversion": "npm run clean && npm run format && npm test",
    "version": "npm run build && git add .",
    "postversion": "git push && git push --tags && npm publish --access private",
    "patch": "npm version patch -m \"[PATCH] version (%s)\"",
    "minor": "npm version minor -m \"[MINOR] version (%s)\"",
    "major": "npm version major -m \"[MAJOR] version (%s)\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/weebuinc/web-kit.git"
  },
  "keywords": [
    "Weebu",
    "Web",
    "Kit",
    "Utilities"
  ],
  "author": "Joseph Eniojukan <joseph@weebu.ca>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/weebuinc/web-kit/issues"
  },
  "homepage": "https://github.com/weebuinc/web-kit",
  "devDependencies": {
    "@babel/preset-env": "7.22.5",
    "@storybook/addon-essentials": "7.0.23",
    "@storybook/addon-links": "7.0.23",
    "@storybook/blocks": "7.0.23",
    "@storybook/web-components": "7.0.23",
    "@storybook/web-components-webpack5": "7.0.23",
    "@types/jest": "29.5.2",
    "jest": "29.5.0",
    "lit": "2.7.5",
    "prettier": "2.8.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "storybook": "7.0.23",
    "ts-jest": "29.1.0",
    "typescript": "5.0.4"
  }
}